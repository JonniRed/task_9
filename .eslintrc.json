{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["eslint: recommended", "plugin: prettier/recommended"],
  "parseOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "indent": ["error", 2],
    "quotes": ["error", "single"],
    "semi": ["error", "always"]
  }
}
